<%= simple_form_for(@member) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :auth_type %>
    <%= f.input :auth_id %>
    <%= f.input :auth_token %>
    <%= f.input :token_created_at %>
    <%= f.input :token_confirmed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
